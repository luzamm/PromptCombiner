<Window x:Class="PromptCombiner.SaveConfig"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PromptCombiner"
        mc:Ignorable="d"
        Title="保存为" Height="166" Width="180" ResizeMode="NoResize">
    <Grid>
        <Button x:Name="saveConfirmButton" Content="保存" HorizontalAlignment="Center" Margin="0,83,0,0" VerticalAlignment="Top" Click="saveConfirmButton_Click"/>
        <Label x:Name="saveNameLabel" Content="文件名" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="saveAsNameTextBox" HorizontalAlignment="Center" Margin="0,51,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
